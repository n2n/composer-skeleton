    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "n2n/n2n": "7.1.x-dev",
        "n2n/n2n-batch": "7.1.x-dev",
        "n2n/n2n-context": "7.1.x-dev",
        "n2n/n2n-impl-persistence-meta": "7.1.x-dev",
        "n2n/n2n-impl-persistence-orm": "7.1.x-dev",
        "n2n/n2n-impl-web-dispatch": "7.1.x-dev",
        "n2n/n2n-impl-web-ui": "7.1.x-dev",
        "n2n/n2n-impl-web-dispatch": "7.1.x-dev",
        "n2n/n2n-composer-skeleton-installer": "7.1.x-dev"
    },
    "extra": {
        "n2n/n2n-composer-skeleton-installer": {
            "optional": {
            	"n2n/rocket": "1.1.x-dev",
